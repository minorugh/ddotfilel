! .Xmodmap for key remap

! remove LOCK = Caps_Lock
! keysym Caps_Lock = Control_L
add Control = Control_L

keycode 107 = Alt_R
keycode  97 = underscore backslash underscore backslash
keycode 66 = Control_L

! 10keys
keycode 87 = 1
keycode 88 = 2
keycode 89 = 3
keycode 83 = 4
keycode 84 = 5
keycode 85 = 6
keycode 79 = 7
keycode 80 = 8
keycode 81 = 9
keycode 90 = 0
keycode 91 = period
